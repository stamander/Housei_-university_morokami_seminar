.wrapper

  .photo-box
    %ul.slider

      = image_tag 'photo1.jpg', alt: 's', class: 'photo1'
      = image_tag 'photo2.jpg', alt: 's', class: 'photo2'
      = image_tag 'photo3.jpg', alt: 's', class: 'photo3'
      = image_tag 'photo4.jpg', alt: 's', class: 'photo4'
      = image_tag 'photo5.jpg', alt: 's', class: 'photo5'
      = image_tag 'photo6.jpg', alt: 's', class: 'photo6'
      
    .photo-box__logo-box 
      .photo-box__logo-box__contents1 MOROKAMI SEMINAR
      = link_to "/front/:id",class: "photo-box__logo-box__contents2" do
        %span<>
        What is Morokami Seminar ? 

  .title information
  .news-box 

    %html
    .iframe-wrap
      %iframe{src: "https://morozemi.ws.hosei.ac.jp/whatsnew.html", class:"info"}
    .news-box__twitter
      %html
      %a.twitter-timeline{:href => "https://twitter.com/morozemi_hosei?ref_src=twsrc%5Etfw"} Tweets by morozemi_hosei
      %script{:async => "", :charset => "utf-8", :src => "https://platform.twitter.com/widgets.js"}


  .tweet
    - @privates.each do |tweet|
      .tweet_title
      = tweet.title

      .tweet_contents
      =tweet.content






:javascript
  $(function(){
    $('.slider').slick({
      arrows: true,
      vertical: false,
      autoplay:true,
      autoplayspeed:500,
    });
  });


