.wrapper

  .photo-box
    %ul.slider

      = image_tag 'photo1.jpg', alt: 's', class: 'photo1'
      = image_tag 'photo2.jpg', alt: 's', class: 'photo2'
      = image_tag 'photo3.jpg', alt: 's', class: 'photo3'
      = image_tag 'photo4.jpg', alt: 's', class: 'photo4'
      = image_tag 'photo5.jpg', alt: 's', class: 'photo5'
      = image_tag 'photo6.jpg', alt: 's', class: 'photo6'
      
    .photo-box__logo-box 
      .photo-box__logo-box__contents1 MOROKAMI SEMINAR
      = link_to "/front/:id",class: "photo-box__logo-box__contents2" do
        %span<>
        What is Morokami Seminar ? 

  .tweet_box 
    .zemiblog zemiNews

    .blog_box
      - @privates.each do |tweet|
        .tweet_title
          = link_to tweet.title, root_path

          
          = link_to 'New', private_path(tweet.id), method: :delete ,class:"deletes"







:javascript
  $(function(){
    $('.slider').slick({
      arrows: true,
      vertical: false,
      autoplay:true,
      autoplayspeed:500,
    });
  });


